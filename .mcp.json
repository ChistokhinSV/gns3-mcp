{
  "mcpServers": {
    "gns3-lab": {
      "command": "python",
      "args": [
        "mcp-server/server/main.py",
        "--host",
        "${GNS3_HOST}",
        "--port",
        "${GNS3_PORT}",
        "--username",
        "${GNS3_USER}",
        "--password",
        "${GNS3_PASSWORD}"
      ],
      "env": {
        "PYTHONPATH": "mcp-server/lib;mcp-server/server"
      }
    }
  }
}
